{
  "name": "linkedin-job-scraper",
  "version": "0.1.0",
  "private": true, 
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev -p 3001",
    "dev:backend": "cd backend && python3 start_backend.py",
    "build": "next build",
    "start": "next start -p 3001",
    "deploy": "npm run build && concurrently \"npm run start\" \"cd backend && python3 start_backend.py\"",
    "lint": "next lint",
    "setup": "npm install && cd backend && python3 start_backend.py"
  },
  "dependencies": {
    "express-rate-limit": "^8.0.1",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
